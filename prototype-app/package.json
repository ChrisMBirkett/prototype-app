{
  "name": "vendor-management",
  "version": "1.1.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --extract-css --base-href /dist/",
    "build": "ng build --aot --extract-css --base-href /dist/",
    "build:watch": "ng build --aot --extract-css --base-href /dist/ --watch",
    "build:publish:prod": "ng build --prod --build-optimizer --base-href /sourcemgmt/",
    "build:publish:qa": "ng build --configuration=qa --build-optimizer --base-href /sourcemgmt/",
    "build:publish:test": "ng build --configuration=test --build-optimizer --base-href /sourcemgmt/",
    "test": "ng test --code-coverage --watch=false",
    "test:watch": "ng test --aot",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "remove-node-modules": "rimraf node_modules",
    "setup": "npm install",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "6.0.3",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "angular2-fontawesome": "0.9.0",
    "bootstrap": "3.3.7",
    "core-js": "2.5.4",
    "fontawesome": "4.7.2",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "log4javascript": "1.4.15",
    "moment": "2.17.1",
    "ng2-toastr": "4.1.2",
    "rxjs": "5.5.11",
    "rxjs-compat": "6.2.1",
    "ts-helpers": "1.1.1",
    "wijmo": "5.20182.500",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/cli": "6.0.5",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.50",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "3.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "standard-version": "^4.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "webpack": "4.4.0"
  }
}
